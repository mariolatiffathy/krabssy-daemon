{
    "name": "Minecraft Latest",
    "author": "Mario Latif",
    "version": "1.0",
    "events": {
        "on_create": {
            "from_container": {
                "1": "wget https://example.com/minecraft.jar",
                "2": "echo \"Downloaded JAR file :)\""
            },
            "as_root": {
                "1": "chattr +u +i ./minecraft.jar"
            }
        }
    }
}